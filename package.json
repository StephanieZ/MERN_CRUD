  },
  "homepage": "https://github.com/StephanieZ/MERN_CRUD#readme",
  "dependencies": {
    "babel-polyfill": "^6.13.0",
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "mongodb": "^2.2.6",
    "source-map-support": "^0.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.9.0",
